  "bugs": {
    "url": "https://github.com/winkjs/wink-bm25-text-search/issues"
  },
  "homepage": "http://winkjs.org/",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "docco": "^0.8.0",
    "eslint": "^5.16.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "standard-changelog": "^2.0.7"
  },
  "dependencies": {
    "wink-nlp-utils": "^2.0.4",
