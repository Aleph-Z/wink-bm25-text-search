    "docs": "docco -o ./docs/ ./src/*.js",
    "lint": "eslint ./src/*.js ./test/*.js ./runkit/*.js",
    "changelog": "standard-changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winkjs/wink-bm25-text-search.git"
  },
